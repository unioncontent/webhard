<div class="main-body">
  <div class="page-wrapper">
    <div class="page-header">
      <div class="page-header-title">
        <h4>모니터링현황</h4>
      </div>
      <div class="page-header-breadcrumb">
        <ul class="breadcrumb-title">
          <li class="breadcrumb-item">
            <a href="/">
              <i class="fas fa-home"></i>
            </a>
          </li>
          <li class="breadcrumb-item"><a href="#!">모니터링현황</a></li>
        </ul>
      </div>
    </div>
    <div class="page-body">
      <div class="row">
        <div class="col-xs-12 top-menu">
          <div class="title-div f-left" style="margin: 16px 0px 0px;">
            <h5 style="margin: 0;">PC/Mobile 현황</h5>
          </div>
          <div class="f-right" id="setting-group">
            <input type="hidden" id="user_class" value="<%=userCLASS%>">
            <input type="hidden" id="user_id" value="<%=userID%>">
            <% if(userCLASS == 'c'){ %>
              <input type="hidden" id="user_mcp" value="<%=userMCP%>">
            <% } %>
            <button class="btn btn-default btn-view btn-square f-right m-l-5 p-r-5 p-l-5" onclick="onPrint()">
              <i class="fas fa-chart-bar"></i>통계보기
            </button>
            <div id="reportrange" class="form-control form-control-default m-l-5 p-b-10 p-r-5 f-right">
              <i class="glyphicon glyphicon-calendar far fa-calendar-alt"></i>
              <span></span>
              <i class="f-right fa fa-angle-down" style="font-size: 17px;"></i>
            </div>
            <% if(userCLASS != 'c'){ %>
            <select name="select" class="col-xs-1 form-control form-control-default m-l-5 p-r-5 f-right" id="selectCP">
              <option value=''>CP사선택</option>
              <% if(userCLASS == 'm'){
                for(var i=0; i < cpList.length; i++) {%>
                  <option value="<%=cpList[i].cp_id%>"><%=cpList[i].cp_cname%></option>
                  <% };
                } %>
              </select>
            <% } %>

            <% if(userCLASS == 'a'){ %>
              <select name="select" class="col-xs-1 form-control form-control-default p-r-5 f-right" id="selectMCP">
                <option value=''>MCP사선택</option>
                <% for(var i=0; i < mcpList.length; i++) {%>
                  <option value="<%=mcpList[i].cp_id%>"><%=mcpList[i].cp_cname%></option>
                  <% }; %>
                </select>
            <% } %>
          </div>
        </div>
        <div class="p-l-15 col-xs-8 m-b-30" id="page-chart1">
          <div class="card">
            <div class="card-type-1 card-type-w1">
              <div class="div-chart">
                <div class="charts_title">
                  PC 모니터링
                </div>
                <div class="charts">
                  <div class="charts__chart chart--p100 chart--lg">
                    <div class="charts__chart chart--p0 chart_point1 f-left"></div>
                    <div class="charts__chart chart--p0 chart_point2 f-left"></div>
                    <div class="charts__chart chart--p0 chart_point3 f-left"></div>
                    <div class="charts__chart chart--p0 chart_point4 f-left"></div>
                  </div>
                </div>
                <div class="charts_legend_group">
                  <div class="chart_legend"><div class="chart_point chart_point4"></div><span class="chart_name">비제휴잔류건 <span class="percent-r">0</span>%</span></div>
                  <div class="chart_legend"><div class="chart_point chart_point3"></div><span class="chart_name">삭제건 <span class="percent-r">0</span>%</span></div>
                  <div class="chart_legend"><div class="chart_point chart_point2"></div><span class="chart_name">제휴전환건 <span class="percent-r">0</span>%</span></div>
                  <div class="chart_legend"><div class="chart_point chart_point1"></div><span class="chart_name">제휴검출건 <span class="percent-r">0</span>%</span></div>
                </div>
              </div>
              <div class="div-chart m-t-30">
                <div class="charts_title">
                  모바일 모니터링
                </div>
                <div class="charts">
                  <div class="charts__chart chart--p100 chart--lg">
                    <div class="charts__chart chart--p0 chart_point1 f-left"></div>
                    <div class="charts__chart chart--p0 chart_point2 f-left"></div>
                    <div class="charts__chart chart--p0 chart_point3 f-left"></div>
                    <div class="charts__chart chart--p0 chart_point4 f-left"></div>
                  </div>
                </div>
                <div class="charts_legend_group">
                  <div class="chart_legend"><div class="chart_point chart_point4"></div><span class="chart_name">비제휴잔류건 <span class="percent-r">0</span>%</span></div>
                  <div class="chart_legend"><div class="chart_point chart_point3"></div><span class="chart_name">삭제건 <span class="percent-r">0</span>%</span></div>
                  <div class="chart_legend"><div class="chart_point chart_point2"></div><span class="chart_name">제휴전환건 <span class="percent-r">0</span>%</span></div>
                  <div class="chart_legend"><div class="chart_point chart_point1"></div><span class="chart_name">제휴검출건 <span class="percent-r">0</span>%</span></div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="p-l-9 col-xs-4" id="page-chart2">
          <div class="card">
            <div class="card-type-1 card-type-w2">
              <table class="table" id="totalTable">
                <thead>
                  <tr>
                    <th width="150px">관리방법</th>
                    <th width="104px">총 검출건</th>
                    <th width="104px">PC</th>
                    <th width="104px">모바일</th>
                  </tr>
                </thead>
                <tbody>
                <tr>
                  <th>전체검출건</th>
                  <td id="totalTable-td1">0</td>
                  <td id="totalTable-p1">0</td>
                  <td id="totalTable-m1">0</td>
                </tr>
                <tr>
                  <th>전체제휴건</th>
                  <td id="totalTable-td2">0</td>
                  <td id="totalTable-p2">0</td>
                  <td id="totalTable-m2">0</td>
                </tr>
                <tr>
                  <th>전체비제휴건</th>
                  <td id="totalTable-td3">0</td>
                  <td id="totalTable-p3">0</td>
                  <td id="totalTable-m3">0</td>
                </tr>
                <tr>
                  <th><span class="card-span">└ 제휴전환건</span></th>
                  <td id="totalTable-td4">0</td>
                  <td id="totalTable-p4">0</td>
                  <td id="totalTable-m4">0</td>
                </tr>
                <tr>
                  <th><span class="card-span">└ 삭제건</span></th>
                  <td id="totalTable-td5">0</td>
                  <td id="totalTable-p5">0</td>
                  <td id="totalTable-m5">0</td>
                </tr>
                <tr>
                  <th><span class="card-span">└ 비제휴잔류</span></th>
                  <td id="totalTable-td6">0</td>
                  <td id="totalTable-p6">0</td>
                  <td id="totalTable-m6">0</td>
                </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class="col-xs-12" id="resultList">
          <div class="row">
            <div class="col-xs-12">
              <div class="btn-xgroup">
                <input type="hidden" name="oc" value="<%=oc%>">
                <button id="bntALL" class="btn btn-primary btn-square ">전체보기</button>
                <button id="bntOSP" class="btn btn-default btn-square form-control form-control-inverse btn-select" style="padding:5px">웹하드사별 통계</button>
                <button id="bntCNT" class="btn btn-default btn-square form-control form-control-inverse">콘텐츠별 통계</button>
                <button class="btn btn-excel form-control form-control-inverse">
                  <i class="far fa-file-excel"></i>엑셀
                </button>
              </div>
              <!-- <select name="select" class="col-xs-1 form-control form-control-inverse m-l-5 p-r-5 f-right" id="selectOC"> -->
              <!-- </select> -->
              <div class="title-div f-left">
                <h5 style="margin-bottom: 0px;margin-top: 15px;">※ 비제휴 평가점수 = ( 비제휴 / 총검출 ) * 비제휴, 1미만 <span class="text-green">●양호</span> /  1~30  <span class="text-blue">●주의</span> / 30 이상 <span class="text-red">●경고</span></h5>
              </div>
            </div>
            <div class="col-xs-12" style="min-height: 400px;">
              <div class="card" id="table-card">
                <div class="card-block table-border-style table-responsive">
                  <div class="preloader3 loader-block">
                    <div class="circ1 loader-primary"></div>
                    <div class="circ2 loader-primary"></div>
                    <div class="circ3 loader-primary"></div>
                    <div class="circ4 loader-primary"></div>
                  </div>
                  <table class="table" id="listTable">
                    <thead>
                      <tr>
                        <th width="1%" rowspan="2">NO</th>
                        <th width="5%" rowspan="2" id="ocnt">
                          <%if(oc == '1'){%>
                            콘텐츠
                          <%} else{%>
                            OSP
                          <%}%>
                        </th>
                        <th width="5%" rowspan="2">비제휴평가</th>
                        <th width="5%" rowspan="2">총검출건</th>
                        <th width="5%" rowspan="2">검출건</th>
                        <th width="5%" rowspan="2">제휴건수</th>
                        <th width="5%" colspan="4" id="colspan-na">비제휴통계</th>
                      </tr>
                      <tr>
                       <th>비제휴건수</th>
                       <th>제휴전환건수</th>
                       <th>삭제건수</th>
                       <th>비제휴잔류건수</th>
                      </tr>
                    </thead>
                    <tbody>
                      <%
                      function numberWithCommas(x) {
                        if(x == undefined){
                          return 0;
                        }
                        return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                      }
                      if(oc == '0'){
                        for(var i=0; i < list.length; i++) {
                          var naNum = (((Number(list[i].natotal.replace(/,/gi,''))+Number(list[i].m_natotal.replace(/,/gi,''))) / (Number(list[i].total.replace(/,/gi,''))+Number(list[i].m_total.replace(/,/gi,''))) ) * (Number(list[i].natotal.replace(/,/gi,''))+Number(list[i].m_natotal.replace(/,/gi,'')))).toFixed(1);
                      %>
                          <tr>
                            <td><%= listCount - i - offset %></td>
                            <td>
                              <div class="title_div">
                                <span class="openModal" data-osp="<%= ((list[i].cnt_osp == null)?list[i].osp_id:list[i].cnt_osp) %>"><%= list[i].osp_sname %></span>
                                <%if(list[i].osp_tstate == '1'){%>
                                  <div class="circle circle-tstate"><span>제휴</span></div>
                                <%}else{%>
                                  <div class="circle circle-tstate circle-tstate-an"><span>비제휴</span></div>
                                <%}%>
                              </div>
                            </td>
                            <td>
                              <div class="title_div">
                                <% if(naNum < 1){ %>
                                  <span class="text-green">● 양호 (<%=naNum%>)</span>
                                <% } else if(naNum >= 1 && naNum < 30){%>
                                  <span class="text-blue">● 주의 (<%=naNum%>)</span>
                                <% } else if(naNum >=30){%>
                                  <span class="text-red">● 경고 (<%=naNum%>)</span>
                                <% } else{ %>
                                  <span class="text-center"><i class="fas fa-minus" style="color: gray;"></i></span>
                                <% } %>
                              </div>
                            </td>
                            <td><%= numberWithCommas(Number(list[i].total.replace(/,/gi,''))+Number(list[i].m_total.replace(/,/gi,''))) %></td>
                            <td style="padding: 0;">
                              <div class="total-result">
                                <div class="total-div total-pc">
                                  <div class="circle circle-platform"><span>PC</span></div>
                                  <span><%= list[i].total %></span>
                                </div>
                                <div class="total-div total-mobile">
                                  <div class="circle circle-platform"><span>Mobile</span></div>
                                  <span><%= list[i].m_total %></span>
                                </div>
                              </div>
                            </td>
                            <td style="padding: 0;border-right: 1px solid orange;">
                              <div class="total-result">
                                <div class="total-div total-pc">
                                  <span><%= list[i].atotal %></span>
                                </div>
                                <div class="total-div total-mobile">
                                  <span><%= list[i].m_atotal %></span>
                                </div>
                              </div>
                            </td>
                            <td style="padding: 0;">
                              <div class="total-result">
                                <div class="total-div total-pc">
                                  <span><%= list[i].natotal %></span>
                                </div>
                                <div class="total-div total-mobile">
                                  <span><%= list[i].m_natotal %></span>
                                </div>
                              </div>
                            </td>
                            <td style="padding: 0;">
                              <div class="total-result">
                                <div class="total-div total-pc">
                                  <span><%= list[i].d_atotal %></span>
                                </div>
                                <div class="total-div total-mobile">
                                  <span><%= list[i].m_d_atotal %></span>
                                </div>
                              </div>
                            </td>
                            <td style="padding: 0;">
                              <div class="total-result">
                                <div class="total-div total-pc">
                                  <span><%= list[i].d_dtotal %></span>
                                </div>
                                <div class="total-div total-mobile">
                                  <span><%= list[i].m_d_dtotal %></span>
                                </div>
                              </div>
                            </td>
                            <td style="padding: 0;">
                              <div class="total-result">
                                <div class="total-div total-pc">
                                  <span><%= list[i].d_natotal %></span>
                                </div>
                                <div class="total-div total-mobile">
                                  <span><%= list[i].m_d_natotal %></span>
                                </div>
                              </div>
                            </td>
                          </tr>
                      <% }
                    } else{
                       for(var i=0; i < list.length; i++) {
                         var naNum = (((Number(list[i].natotal.replace(/,/gi,''))+Number(list[i].m_natotal.replace(/,/gi,''))) / (Number(list[i].total.replace(/,/gi,''))+Number(list[i].m_total.replace(/,/gi,''))) ) * (Number(list[i].natotal.replace(/,/gi,''))+Number(list[i].m_natotal.replace(/,/gi,'')))).toFixed(1);
                    %>
                         <tr>
                           <td><%= listCount - i - offset %></td>
                           <td>
                             <div class="title_div title_div_c">
                               <span class="cnt"><%= list[i].cnt_title %></span>
                            </div>
                           </td>
                           <td>
                             <div class="title_div">
                               <% if(naNum < 1){ %>
                                 <span class="text-green">● 양호 (<%=naNum%>)</span>
                               <% } else if(naNum >= 1 && naNum < 30){%>
                                 <span class="text-blue">● 주의 (<%=naNum%>)</span>
                               <% } else if(naNum >=30){%>
                                 <span class="text-red">● 경고 (<%=naNum%>)</span>
                               <% } else{ %>
                                 <span class="text-center"><i class="fas fa-minus" style="color: gray;"></i></span>
                               <% } %>
                             </div>
                           </td>
                           <td><%= numberWithCommas(Number(list[i].total.replace(/,/gi,''))+Number(list[i].m_total.replace(/,/gi,''))) %></td>
                           <td style="padding: 0;">
                             <div class="total-result">
                               <div class="total-div total-pc">
                                 <div class="circle circle-platform"><span>PC</span></div>
                                 <span><%= list[i].total %></span>
                               </div>
                               <div class="total-div total-mobile">
                                 <div class="circle circle-platform"><span>Mobile</span></div>
                                 <span><%= list[i].m_total %></span>
                               </div>
                             </div>
                           </td>
                           <td style="padding: 0;border-right: 1px solid orange;">
                             <div class="total-result">
                               <div class="total-div total-pc">
                                 <span><%= list[i].atotal %></span>
                               </div>
                               <div class="total-div total-mobile">
                                 <span><%= list[i].m_atotal %></span>
                               </div>
                             </div>
                           </td>
                           <td style="padding: 0;">
                             <div class="total-result">
                               <div class="total-div total-pc">
                                 <span class="natotal" data-type="pc" data-cid="<%= list[i].cnt_id %>"><%= list[i].natotal %></span>
                               </div>
                               <div class="total-div total-mobile">
                                 <span class="natotal" data-type="mobile" data-cid="<%= list[i].cnt_id %>"><%= list[i].m_natotal %></span>
                               </div>
                             </div>
                           </td>
                           <td style="padding: 0;">
                             <div class="total-result">
                               <div class="total-div total-pc">
                                 <span><%= list[i].d_atotal %></span>
                               </div>
                               <div class="total-div total-mobile">
                                 <span><%= list[i].m_d_atotal %></span>
                               </div>
                             </div>
                           </td>
                           <td style="padding: 0;">
                             <div class="total-result">
                               <div class="total-div total-pc">
                                 <span><%= list[i].d_dtotal %></span>
                               </div>
                               <div class="total-div total-mobile">
                                 <span><%= list[i].m_d_dtotal %></span>
                               </div>
                             </div>
                           </td>
                           <td style="padding: 0;">
                             <div class="total-result">
                               <div class="total-div total-pc">
                                 <span><%= list[i].d_natotal %></span>
                               </div>
                               <div class="total-div total-mobile">
                                 <span><%= list[i].m_d_natotal %></span>
                               </div>
                             </div>
                           </td>
                         </tr>
                      <% }
                    } %>
                    </tbody>
                    <tfoot>
                      <%if(oc == '1'){
                        var limit = 20;
                        var pageCount = Math.ceil(listCount/limit);
                        if(pageCount > 1) {
                        %>
                        <tr class="footable-paging">
                          <td colspan="10">
                            <ul class="pagination">
                              <%
                                var pageSize = 5;
                                var pRCnt = parseInt(page / pageSize);
                                if(page % pageSize == 0){
                                  pRCnt = parseInt(page / pageSize) - 1;
                                }
                                if(page > 5) {
                              %>
                                <li class="page-item">
                                  <a class="page-link" data-value="1" aria-label="Previous">
                                    <i class="ti-angle-double-left f-12"></i>
                                    <span aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                  </a>
                                </li>
                                <li class="page-item">
                                  <a class="page-link" data-value=="<%=(pRCnt * pageSize)%>" aria-label="Previous">
                                    <i class="ti-angle-left f-12"></i>
                                    <span aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                  </a>
                                </li>
                              <% } %>
                              <% for(var index=pRCnt * pageSize + 1;index<(pRCnt + 1)*pageSize + 1;index++){%>
                                <li class= "<%=(page == index) ? 'active' : ''%> page-item">
                                  <a class="page-link" data-value="<%=index%>" >
                                    <%= index %>
                                  </a>
                                </li>
                              <%
                                  if(index == pageCount) {
                                    break;
                                  }
                                }
                              %>
                              <% if((pRCnt + 1) * pageSize < pageCount) { %>
                                <li class="page-item">
                                  <a class="page-link" data-value="<%=(pRCnt + 1)*pageSize+1%>" aria-label="Next">
                                    <i class="ti-angle-right f-12"></i>
                                    <span aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                  </a>
                                </li>
                                <li class="page-item">
                                  <a class="page-link" data-value="<%=pageCount%>" aria-label="Next">
                                    <i class="ti-angle-double-right f-12"></i>
                                    <span aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                  </a>
                                </li>
                              <% } %>
                            </ul>
                          </td>
                        </tr>
                        <% }
                      } %>
                    </tfoot>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- Modal start-->
        <div class='modal fade' id='num-Modal' aria-hidden="true" tabindex="-1" role="dialog">
          <div class="modal-dialog modal-xxlg" role="document" style="">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true"><i class="ti-close"></i></span>
                </button>
              </div>
              <div class="modal-body p-t-0">
                <div class="col-xs-12">
                  <div class="row">
                    <div class="col-xs-12 m-b-10">
                      <div class="col-xs-6 f-right" style="padding: 0;">
                        <input type="text" class="f-left form-control m-r-5 p-t-10 form-control-inverse search-text" id="searchInput-nm" style="width: 359px;">
                        <button id="searchBtn-nm" class="btn btn-inverse btn-square btn-search f-right">검색</button>
                      </div>
                      <select id="selectSearchType-nm" name="select" class="col-xs-1 form-control form-control-inverse m-r-5 f-right search-select">
                        <option value="t">제목</option>
                        <option value="n">게시물번호</option>
                        <option value="k">키워드</option>
                      </select>
                    </div>
                    <div class="col-xs-12" id="resultList-nm">
                      <div class="card">
                        <div class="card-block table-border-style table-responsive">
                          <div class="preloader3 loader-block">
                            <div class="circ1 loader-primary"></div>
                            <div class="circ2 loader-primary"></div>
                            <div class="circ3 loader-primary"></div>
                            <div class="circ4 loader-primary"></div>
                          </div>
                          <table class="table">
                            <thead>
                              <tr>
                                <th width="1%">NO</th>
                                <th width="5%">플랫폼</th>
                                <th width="5%">OSP</th>
                                <th width="5%">MCP/CP</th>
                                <th width="5%">콘텐츠 제목</th>
                                <th width="15%">제목</th>
                                <th width="5%">판매금액</th>
                                <th width="5%">판매자</th>
                                <th width="5%">검출상태</th>
                                <th width="5%">검출일</th>
