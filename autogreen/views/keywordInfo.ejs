<script>
// url 변경 후 파라미터값 가져오기
var url_string = window.location.href
var url = new URL(url_string);

//페이지 변경시
$(document).on('click','.page-link',function(){
  var num = Number($(this).text()) || Number($(this).data().num);
  if(url.searchParams.get("cp") == '' || url.searchParams.get("cp") == null){
    return false;
  }
  var param = {
    cp : url.searchParams.get("cp"),
    start : Number($(this).data().value)
  };
  if(url.searchParams.get("searchType") != '' && url.searchParams.get("searchType") != null){
    param.searchType = url.searchParams.get("searchType");
  }
  if(url.searchParams.get("search") != '' && url.searchParams.get("search") != null){
    param.search = url.searchParams.get("search");
  }
  ajaxGetPageList(param,num);
});

// 모달 수정
$(document).on('change','.k_key_m, .k_apply_m, .k_method_m',function(){
  if($('#modal-cntList > tbody tr').length > 1){
    KeywordStatusUpdateAjax();
  }
  else{
    $(this).parents('.form-radio').children().last().val(this.value);
  }
});

// 모달 삭제 버튼 클릭시
$(document).on("click",".remove_keyword",function(){
  var idx = $(this).parents("tr").find(".n_idx_k").val();
  $.ajax({
    url: '../keyword/delete',
    type: 'post',
    data: {
      'n_idx': idx
    },
    error:function(request,status,error){
      alert("다시시도해주세요.");
      console.log('code:'+request.status+'\n'+'message:'+request.responseText+'\n'+'error:'+error);
    },
    success:function(data){
      if(data){
        reloadKeyList($("#cntIdx_m").val());
      }
      else{
        alert("다시시도해주세요.");
      }
    }
  });
});

//키워드 추가
$("#insertKeywordBtn").on("click",function(){
  $.ajax({
    url: '../keyword/add',
    type: 'post',
    data: {
      'keyword': [$('#pSearch').val(),$('#dSearch').val()],
      'n_idx_c': $("#cntIdx_m").val(),
      'K_key': $("input[name ='oK_key']").val(),
      'K_method': $("input[name ='oK_method']").val(),
      'K_apply': $("input[name ='oK_apply']").val(),
      'delay_time': $('input[name=delay_time]').val(),
      'U_id_c': url.searchParams.get("cp"),
    },
    error:function(request,status,error){
      alert("다시시도해주세요.");
      console.log('code:'+request.status+'\n'+'message:'+request.responseText+'\n'+'error:'+error);
    },
    success:function(data){
      if(data){
        console.log(data);
        alert("추가되었습니다.");
        $('#pSearch').val("");
        $('#dSearch').val("");
        reloadKeyList(data.n_idx_c);
      }
    }
  });
});

// 검색시
$('#searchBtn').on('click',function(){
  if($("#keywordInput").val() == ''){
    alert('검색어를 넣어주세요.');
    return false;
  }
  location.href = '/keyword/info?page=1&cp='+url.searchParams.get("cp")+'&searchType='+$("#selectSearchType option:selected").val()+'&search='+$("#keywordInput").val();
});
$('#keywordInput').on('keyup',function(event){
  if(event.keyCode == 13){
    $('#searchBtn').trigger('click');
  };
});

// 수정 버튼 클릭시
$(document).on('click','.btn-update',function(){
  var trEle = $(this).closest("tr");
  $("#m_title").text(trEle.find('td').eq(0).text());
  reloadKeyList(trEle.find('input').val());
  $("#keyword-Modal").modal('show');
});

// 삭제 버튼 클릭시
$(document).on("click",".btn-delete",function(){
  var idx = $(this).parents("tr").find("input").val();
  $.ajax({
    url: '../keyword/delete',
    type: 'post',
    data: {
      'n_idx': idx,
      'type': 'c_idx'
    },
    error:function(request,status,error){
      alert("다시시도해주세요.");
      console.log('code:'+request.status+'\n'+'message:'+request.responseText+'\n'+'error:'+error);
    },
    success:function(data){
      alert("삭제되었습니다.");
      var param = {
        cp : url.searchParams.get("cp"),
        start : 0
      };
      var num = 1;
      if($('li.page-item.active > a').data() != undefined){
        param.start = $('li.page-item.active > a').data().value;
        num = Number($('li.page-item.active > a').text());
      }
      if(url.searchParams.get("searchType") != '' && url.searchParams.get("searchType") != null){
        param.searchType = url.searchParams.get("searchType");
      }
      if(url.searchParams.get("search") != '' && url.searchParams.get("search") != null){
        param.search = url.searchParams.get("search");
      }
      ajaxGetPageList(param,num);
    }
  });
});

//작업Delay시간 수정시
$( "#delay_time" ).blur(function(){
  if($('input[name=oDelay_time]').val() != $('#delay_time').val()){
    if($('#delay_time').val() == ''){
      $('#delay_time').val('0');
    }
    KeywordStatusUpdateAjax();
    console.log( "작업Delay시간 수정" );
  }
});

function KeywordStatusUpdateAjax(){
  $.ajax({
    url: '../contents/update',
    type: 'post',
    data: {
      'K_method': $('input:radio[name=k_method_m]:checked').val(),
      'K_key': $('input:radio[name=k_key_m]:checked').val(),
      'K_apply': $('input:radio[name=k_apply_m]:checked').val(),
      'delay_time': $('input[name=delay_time]').val(),
      'n_idx_c': $("#cntIdx_m").val()
    },
    error:function(request,status,error){
      alert("다시 시도 해주세요.");
      console.log('code:'+request.status+'\n'+'message:'+request.responseText+'\n'+'error:'+error);
    },
    success:function(data){
      if(data){
        // console.log(data);
        $("input[name ='oK_key']").val(data.K_key);
        $("input[name ='oK_method']").val(data.K_method);
        $("input[name ='oK_apply']").val(data.K_apply);
        $("input[name ='oDelay_time']").val(data.delay_time);
        $("#delay_time").val(data.delay_time);
        $("#cntIdx_m").val(data.n_idx_c);
        $("input:radio[name ='k_key_m']:input[value='"+data.K_key+"']").prop("checked", true);
        $("input:radio[name ='k_method_m']:input[value='"+data.K_method+"']").prop("checked", true);
        $("input:radio[name ='k_apply_m']:input[value='"+data.K_apply+"']").prop("checked", true);
        alert("수정되었습니다.");
      }
    }
  });
}

function reloadKeyList(cntIdx){
  $("#cntIdx_m").val(cntIdx);
  $.ajax({
    url: '../keyword/searchKeyInfo',
    type: 'post',
    data : {'n_idx_c': cntIdx},
    error:function(request,status,error){
      // alert('code:'+request.status+'\n'+'message:'+request.responseText+'\n'+'error:'+error);
      alert(request.responseText);
    },
    success:function(data){
      // console.log(data);
      $("#modal-cntList tbody tr").not("#inputKey").remove()
      data.forEach(function(item, idx){
        if(idx == 0){
          $("input[name ='oK_key']").val(item.K_key);
          $("input[name ='oK_method']").val(item.K_method);
          $("input[name ='oK_apply']").val(item.K_apply);
          $("input[name ='oDelay_time']").val(data.delay_time);
          $("input[name ='delay_time']").val(item.delay_time);
          $("input:radio[name ='k_key_m']:input[value='"+item.K_key+"']").prop("checked", true);
          $("input:radio[name ='k_method_m']:input[value='"+item.K_method+"']").prop("checked", true);
          $("input:radio[name ='k_apply_m']:input[value='"+item.K_apply+"']").prop("checked", true);
        }
        var html = '<tr><th scope="row" class="centerTh">'+(idx+1)+'</th>';
        if(item.K_type != '0'){
          html += '<td><div class="keyword_nobr">'+item.K_keyword+'</div><input type="hidden" class="n_idx_k" value="'+item.n_idx+'"/></td><td></td>';
        }
        else{
          html += '<td></td><td><div class="keyword_nobr">'+item.K_keyword+'</div><input type="hidden" class="n_idx_k" value="'+item.n_idx+'"/></td>';
        }
        html += '<td><button class="remove_keyword btn btn-danger btn-sm"><i class="icofont icofont-garbage" style="margin-right:0"></i></button></td></tr>';
        $("#inputKey").after(html);
      });
    }
  });
}

function ajaxGetPageList(param,pageNum){
  $.ajax({
    url: '/keyword/getNextPage',
    type: 'post',
    data: param,
    error: function(request,status,error){
      console.log('code:'+request.status+'\n'+'message:'+request.responseText+'\n'+'error:'+error);
      alert('새로고침해주세요~');
    },
    success: function(data){
      // console.log(data);
      pageNum = pageNum || data.pageCount;
      if(typeof(history.pushState) == 'function'){
        //현재 주소를 가져온다.
        var renewURL = location.href;
        //현재 주소 중 page 부분이 있다면 새로 부여될 페이지 번호를 할당한다.
        renewURL = renewURL.replace(/\?page=([0-9]+)/ig,'?page='+pageNum);
        if(renewURL.indexOf("?page=") == -1){
          renewURL += '?page='+pageNum;
        }
        //페이지 갱신 실행!
        history.pushState(null, null,renewURL);
      }
      $('.col-md-12 tfoot').empty();
      if(data.pageCount > 1) {
        var html = '<tr><td colspan="7"><ul class="pagination float-right">';
        var y = 1;
        if(pageNum > 1  && data.pageCount > 4) {
          y = pageNum-1;
          html += '<li class="page-item"><a class="page-link" data-value="0" data-num="1" aria-label="Previous">&laquo;\
              <span aria-hidden="true"></span>\
              <span class="sr-only">Previous</span>\
            </a></li>';
        }

        for (y; y <= data.pageCount; y++) {
          var active = (pageNum == y) ? "active" : "";
          html += '<li class=\"'+active+' page-item">\
            <a class="page-link" data-value=\"'+((y-1) * 50)+'"\ >'+y+'</a></li>'

          if(parseInt(pageNum) + 3 == y) {
            break;
          }
        }
        if(pageNum != data.pageCount && data.pageCount > 4) {
          html += '<li class="page-item">\
                <a class="page-link" data-value=\"'+(Math.floor(data.total/50)*50)+'"\ aria-label="Next">&raquo;\
                  <span aria-hidden="true"></span>\
                  <span class="sr-only">Next</span>\
                </a></li>';
        }
        html += '</ul></td></tr>';
        $('.col-md-12 tfoot').append(html);
      }

      $('.col-md-12 tbody').empty();
      // console.log(data);
      data.kcList.forEach(function(item,idx){
        var numIdx = Math.ceil(data.total-(param.start+idx)).toString();
        $('.col-md-12 tbody').append('<tr>\
        <input name="cntIdx" type="hidden" class="form-control" value="'+item.n_idx_c+'"/>\
        <th>'+numIdx+'</th>\
          <td><div class="title_nobr">'+item.CP_title+'</div></td>\
          <td><div class="cp_nobr">'+item.CP_CntID+'</div></td>\
          <td>'+item.K_type_a+' 개</td>\
          <td>'+item.K_type_d+' 개</td>\
          <td>\
            <div class="form-radio">\
              <div class="radio radio-inline">\
                <label>\
                  <input type="radio" value="D" name=\"k_apply'+idx+'\" '+((item.K_apply == 'D') ? 'checked' : '')+' disabled>\
                  <i class="helper"></i>삭제\
                </label>\
              </div>\
              <div class="radio radio-inline">\
                <label>\
                  <input type="radio" value="T" name=\"k_apply'+idx+'\" '+((item.K_apply == 'T') ? 'checked' : '')+' disabled>\
                  <i class="helper"></i>제휴\
                </label>\
              </div>\
              <div class="radio radio-inline">\
                <label>\
                  <input type="radio" value="P" name=\"k_apply'+idx+'\" '+((item.K_apply == 'P') ? 'checked' : '')+' disabled>\
                  <i class="helper"></i>보류\
                </label>\
              </div>\
            </div>\
          </td>\
          <td>\
            <div class="btn-group btn-group-md text-center" style="padding-right: 0; display: inline-block;">\
              <button type="button" class="btn-delete btn btn-danger waves-effect waves-light alert-confirm1" style="margin-right: 5px;">\
                <span class="icofont icofont-ui-delete"></span>\
              </button>\
              <button type="button" class="btn-update btn btn-primary waves-effect waves-light">\
                <span class="icofont icofont-ui-edit"></span>\
              </button>\
            </div>\
          </td>\
        </tr>');
      });
    }
  });
}
</script>
