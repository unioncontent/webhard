                        <div class="radio radio-inline">
                          <label>
                            <input type="radio" value="D" name="k_apply<%= i %>" <%= (kcList[i].K_apply == 'D') ? 'checked' : '' %> disabled>
                            <i class="helper"></i>삭제
                          </label>
                        </div>
                        <div class="radio radio-inline">
                          <label>
                            <input type="radio" value="T" name="k_apply<%= i %>" <%= (kcList[i].K_apply == 'T') ? 'checked' : '' %> disabled>
                            <i class="helper"></i>제휴
                          </label>
                        </div>
                        <div class="radio radio-inline">
                          <label>
                            <input type="radio" value="P" name="k_apply<%= i %>" <%= (kcList[i].K_apply == 'P') ? 'checked' : '' %> disabled>
                            <i class="helper"></i>보류
                          </label>
                        </div>
                      </div>
                    </td>
                    <td>
    								  <div class="btn-group btn-group-md text-center" style="padding-right: 0; display: inline-block;">
                        <button type="button" class="btn-delete btn btn-danger waves-effect waves-light alert-confirm1" style="margin-right: 5px;">
        								  <span class="icofont icofont-ui-delete"></span>
        								</button>
      							    <button type="button" class="btn-update btn btn-primary waves-effect waves-light">
        								  <span class="icofont icofont-ui-edit"></span>
        								</button>
    								  </div>
    								</td>
                  </tr>
                  <% }; %>
                </tbody>
                <tfoot>
                  <% if(pageCount > 1) { %>
                  <tr>
                    <td colspan="7">
                      <ul class="pagination float-right">
                        <%
                          var y = 1;
                          if(currentPage > 1 && pageCount > 4) {
                            y = currentPage-1;
                        %>
                          <li class="page-item">
                            <a class="page-link" data-value="0" data-num="1" aria-label="Previous">
                              &laquo;
                              <span aria-hidden="true"></span>
                              <span class="sr-only">Previous</span>
                            </a>
                          </li>
                        <% } %>
                        <% for (y; y <= pageCount; y++) { %>
                          <li class= "<%=(currentPage == y) ? 'active' : ''%> page-item">
                            <a class="page-link" data-value="<%=Math.ceil(((y - 1) * 50)).toString()%>" >
                              <%= y %>
                            </a>
                          </li>
                        <%
                            if(parseInt(currentPage) + 3 == y) {
                              break;
                            }
                          }
                        %>
                        <% if(currentPage != pageCount && pageCount > 4) { %>
                          <li class="page-item">
                            <a class="page-link" data-value="<%=Math.ceil(((pageCount - 1) * 50)).toString()%>" data-num="<%=pageCount%>" aria-label="Next">
                              &raquo;
                              <span aria-hidden="true"></span>
                              <span class="sr-only">Next</span>
                            </a>
                          </li>
                        <% } %>
                      </ul>
                    </td>
                  </tr>
                  <% } %>
